CREATE TABLESPACE test DATAFILE '/home/oracle/app/oracle/oradata/itrek/test' SIZE 512M;
CREATE USER test IDENTIFIED BY test;
grant "RESOURCE" to test;
grant "CONNECT" to test;
grant create session,create table,create user,create view, create sequence to test;

GRANT CREATE ANY SYNONYM TO test;
GRANT CREATE ANY VIEW TO test;

CREATE PUBLIC SYNONYM DROP PUBLIC SYNONYM TO RESOURCE;

alter user test quota unlimited on users;

alter user test
DEFAULT TABLESPACE test
QUOTA UNLIMITED ON test
TEMPORARY TABLESPACE temp01;
>SELECT name FROM v$datafile;  
